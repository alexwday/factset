  "stage_1": {
    "sync_date_range": 1,
    "description": "Daily incremental sync of new transcripts"
  },
  "stage_2": {
    "master_database_path": "Outputs/Database/master_database.json",
    "refresh_output_path": "Outputs/Refresh",
